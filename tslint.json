
{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console":false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-line-length": [
            true,
            140
        ],
        "object-literal-shorthand": [
            false
        ],
        "object-literal-sort-keys": false,
        "trailing-comma": "never",
        "arrow-parens": [true, "ban-single-arg-parens"],
        "member-access": [true, "no-public"]
    },
    "rulesDirectory": []
}